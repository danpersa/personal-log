<div class="head-container">
  <div class="title-container">
    <h1>My idea lists</h1>
  </div>
</div>

<div class="toolbar-container">
  <div class="toolbar-row">
    <div class="well">
      <%= link_to "Create new idea list", new_idea_list_path, 
        :remote => true, 
        :class => "btn small primary" %>
    </div>
  </div>
</div>

<div id="table-div">
  <%= render 'table' %>
</div>

<div id="idea_list_dialog" title="Edit idea list" ></div>

<% content_for :sidebar do %>
  <%= render 'shared/sidebar' %>
<% end %>
