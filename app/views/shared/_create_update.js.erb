<% if @model.errors.any? %>
  <%= render "form_update" %>
<% else %>
  <% if @update_table.nil? or @update_table %>
    <%= render(:partial => @update_table_partial.nil? ? "table_update" : @update_table_partial) %>
  <% end %>
  addNotificationSuccess("<%= escape_javascript(flash[:success]) %>");
  $(dialog_selector).dialog("close");
  $(dialog_selector).dialog("destroy");
<% end %>